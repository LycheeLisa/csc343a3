<qualified>{
  let $parsed := fn:doc("q3.xml")
  for $candidate in ($parsed//resume)
  where count($parsed//resume[@rID/string() = $candidate/@rID/string()]//skill/@what) >= 3
  return ($candidate//forename, $candidate//citizenship, <numskills>{count($parsed//resume[@rID/string() = $candidate/@rID/string()]//skill/@what)}</numskills>)
}</qualified>
